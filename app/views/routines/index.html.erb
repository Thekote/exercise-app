<h1>Routines</h1>

<div id="routines">
  <table>
    <thead>
      <tr>
        <th><strong>Name</strong></th>  
      </tr>
    </thead>
    <tbody>      
      <% @routines.each do |routine| %>
        <tr>
          <td><%= routine.name %></td>
          <td><%= link_to "show routine", routine %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to "New routine", new_routine_path %>  
</div>
